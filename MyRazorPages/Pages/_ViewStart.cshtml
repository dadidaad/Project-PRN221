@{
    Boolean checkLayout = false;
    @if (Context.Session.Get("JWToken") != null ){
        if (Context.User.Claims.First(c => c.Type == ClaimTypes.Email).Value.Equals(UserRoles.Employee))
        {
            checkLayout = true;
        }
    }
    if(checkLayout){
        Layout = "_Layout2";
    }
    else{
        Layout = "_Layout1";
    }
}

